
            <!DOCTYPE html>
            <html>
            <head>
                <title>Mini projet Equipe de personnes</title>
                <meta charset="utf-8">
                <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
                      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
                <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
                      integrity="sha512-iBBXm8fW90+nuLcSKlbmrPcLa0OT92xO1BIsZ+ywDWZCvqsWgccV3gFoRBv0z+8dLJgyAHIhR35VZc2oM/gI1w==" crossorigin="anonymous"/>

                <style>
                    .divListUser,.divUser{
                        background-color: #ccc;
                        padding: 20px;
                    }
                    .divListUser{
                        margin-top:20px;
                    }

                    .divTeam{
                        background-color: #ccc;
                        padding: 20px;
                    }
                    #tabListUser td{ height: 45px; width:120px;}


                </style>
            </head>
            <body>
            <div class="container mt-3">

                <div class="row">
                    <div class="col-md-8">
                        <div class="divTeam">
                            <!-- <form action="page.php" method="POST" > -->
                            {{ form_start(equipeform,{'action':path('equipe_new')}) }}
                            <div class="row">
                                <div class="col-4">
                                    {{ form_widget(equipeform)}}
                                </div>
                                <div class="col-8">
                                    <button class="btn btn-success" type="submit">
                                        <i class="fa fa-plus"></i>
                                    </button>
                                </div>

                            </div>
                            {{ form_end(equipeform)}}


                        </div>
                        <div id="demo">
                            <table class="table table-striped">
                                <tbody>
                                {% for equipe in equipes %}
                                <tr>
                                    <td colspan="3">
                                        <h3>
                                            {{ equipe.nom }}
                                            <button href="{{ path('equipe_delete', {'id': 'equipe.id'}) }}" class="btn btn-primary">del</button>
                                        </h3>
                                    </td>

                                </tr>

                                <tr>
                                    <th>Pr√©nom</th>
                                    <th>Nom</th>
                                    <th>Action</th>

                                </tr>
                                {% for personne in equipe.personnes %}
                                <tr>
                                    <td>{{ personne.prenom }}</td>
                                    <td>{{ personne.nom }}</td>

                                    <td><button href="{{ path('personne_delete', {'id': 'personne.id'}) }}" class="btn btn-primary">del</button></td>
                                </tr>
                                {% endfor %}
                                </tbody>
                                {% endfor %}
                            </table>

                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="divUser">
                            <table>
                                <tr>
                                    {{ form_start(personneform,{'action':path('personne_new')}) }}
                                    {{ form_widget(personneform)}}
                                    <td>
                                        <button class="btn btn-success" type="submit">
                                            <i class="fa fa-plus"></i>
                                        </button>
                                    </td>
                                </tr>
                                {{ form_end(personneform)}}
                            </table>
                        </div><!-- divUser -->



                        <div class="divListUser">

                            <table id="tabListUser">

                            </table>
                        </div>

                    </div>	<!-- col4 -->
                </div><!-- row -->

            </div><!-- container -->

            </body>
            </html>
